{
  "name": "sedona",
  "author": "Nemtsev Konstantin <nemtsevk@hotmail.com> (https://github.com/nemtsev-k)",
  "description": "Личный проект «Седона» от HTML Academy",
  "homepage": "https://nemtsev-k.github.io/sedona/",
  "private": true,
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "3.0.0",
    "autoprefixer": "10.4.19",
    "browser-sync": "3.0.2",
    "del": "7.1.0",
    "gh-pages": "6.1.1",
    "gulp": "5.0.0",
    "gulp-dart-sass": "1.1.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "9.1.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "10.0.0",
    "gulp-rename": "2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-stacksvg": "3.0.0",
    "gulp-svgmin": "4.1.0",
    "gulp-terser": "2.1.0",
    "gulp-webp": "5.0.0",
    "postcss": "8.4.38",
    "postcss-csso": "6.0.1",
    "stylelint": "16.5.0",
    "stylelint-config-htmlacademy": "4.2.1"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "lint": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/images/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "20.13.1"
  },
  "type": "module",
  "volta": {
    "node": "20.13.1"
  }
}
